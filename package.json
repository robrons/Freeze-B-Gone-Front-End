{
  "name": "antifreeze-front-end",
  "version": "0.0.0",
  "description": "Front-end for AntiFreeze",
  "main": "src/index.jsx",
  "repository": "git@github.com:NilsG-S/antifreeze-front-end.git",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --env=dev"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8.9"
          }
        }
      ],
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "no-unused-expressions": 0,
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "jsx-quotes": [
        2,
        "prefer-single"
      ]
    }
  },
  "dependencies": {
    "loglevel": "^1.6.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2", 
    "material-ui": "^material-ui@next"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  }
}
